        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DailyEng">
        <activity android:name=".MainActivity" android:label="Daily English">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".quizActivity" android:label="메인 메뉴"></activity>
        <activity android:name=".leaderboardActivity" android:label="오늘의 기록"></activity>
        <activity android:name=".addEngActivity" android:label="영단어 목록 및 추가"></activity>
        <activity android:name=".dailyQuizActivity" android:label="오늘의 퀴즈"></activity>
        <activity android:name=".result" android:label="퀴즈 결과"></activity>
    </application>

</manifest>